    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/zephyr/zephyr.elf",
            "args": [],
            "stopAtEntry": true,
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerServerAddress": "localhost:3333",
            "miDebuggerPath": "${userHome}/zephyr-sdk-0.16.8/arm-zephyr-eabi/bin/arm-zephyr-eabi-gdb",
            "useExtendedRemote": true,
            "cwd": "${workspaceFolder}",
            "logging": {
                "engineLogging":true,
                "trace": false,
                "traceResponse": false,
                "programOutput": false,
                "moduleLoad": false,
                "exceptions": false
            }
         }
    ]
}
